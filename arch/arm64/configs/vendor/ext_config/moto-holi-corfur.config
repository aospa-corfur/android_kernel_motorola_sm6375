# CONFIG_TOUCHSCREEN_FTS is not set
# CONFIG_TOUCHSCREEN_FTS_DIRECTORY is not set
CONFIG_AF_NOISE_ELIMINATION=y
# CONFIG_TOUCHSCREEN_NT36XXX is not set
# CONFIG_MICRON_UFSHID is not set
CONFIG_CAM_SENSOR_PROBE_RETRY=y
CONFIG_SENSORS_CLASS=m
CONFIG_MMI_ANNOTATE=m
CONFIG_MMI_INFO=m
CONFIG_MMI_RELAY=m
CONFIG_MOTO_F_USBNET=m
CONFIG_WATCHDOGTEST=m
CONFIG_REGULATOR_WL2864C=m
CONFIG_REGULATOR_WL2866D=m
CONFIG_MMI_PARALLEL_CHARGER_IIO=m
CONFIG_MMI_SC8549=m
CONFIG_MMI_SMBCHARGER_IIO=m
CONFIG_QPNP_ADAPTIVE_CHARGE=m
CONFIG_NFC=y
CONFIG_NFC_ST21NFC=m
CONFIG_MMI_SYS_TEMP=m
CONFIG_SARSENSOR_SX937X=m
CONFIG_MOTO_UTAGS=m
CONFIG_FM_ELNA=m
CONFIG_CAMERA_CCI_INTF=m
CONFIG_INPUT_FINGERPRINT=y
CONFIG_FINGERPRINT_FPC_TEE_MMI=m
CONFIG_FINGERPRINT_GOODIX_FOD_MMI=m
CONFIG_PANEL_NOTIFICATIONS=y
CONFIG_INPUT_TOUCHSCREEN_MMI=m
CONFIG_TS_KERNEL_USE_GKI=y
CONFIG_TOUCHCLASS_MMI_MULTIWAY_UPDATE_FW=y
CONFIG_TOUCHSCREEN_FTS_V2_MMI=m
CONFIG_INPUT_FOCALTECH_V2_MMI_IC_NAME_FT3518U=y
# CONFIG_INPUT_FOCALTECH_V2_MMI_IC_NAME_FT5452I is not set
CONFIG_TOUCHSCREEN_FTS_V2_MMI_GESTURES=y
CONFIG_TOUCHSCREEN_FTS_V2_MMI_ENABLE_DOUBLE_TAP=y
CONFIG_TOUCHSCREEN_FTS_V2_MMI_ENABLE_PALM=y
CONFIG_TOUCHSCREEN_FTS_V2_MMI_PEN_NOTIFIER=y
CONFIG_TOUCH_PANEL_NOTIFICATIONS=y
CONFIG_DRM_PANEL_NOTIFICATIONS=y
CONFIG_MSM_DRM_NOTIFICATIONS=y
